{% for member in members %}{% if member.photo is defined %}
<li class="col-md-2 col-sm-3 col-xs-1 isotope-item leadership">
	<span class="thumb-info thumb-info-hide-wrapper-bg mb-xlg">
		<span class="thumb-info-wrapper" style="height:150px">
			<img src="{{ member.photo.photo_link }}" class="img-responsive" alt="" height="150">
			<span class="thumb-info-title">
				<span class="thumb-info-inner">{{ member.name }}</span>
			</span>
		</span>
	</span>
</li>
{% endif %}{% endfor %}